[user]
    name = Zamith
    email = zamith.28@gmail.com
[gui]
    recentrepo = /Users/luis/NetBeansProjects/TSLEditorProject/tsleditor
[dude]
    icon = /Users/luis/Desktop/octocat.png
    interval = 60
[alias]
    lg = log --color --graph --date=short --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%ad) %C(blue)[%an]%Creset' --abbrev-commit
    logme = !git lg --author=zamith
    conflicts = diff --name-only --diff-filter=U
    delete = branch -D
    co = checkout
    ct = commit -v
    today = log --since=midnight --author='Zamith' --oneline
    cont = rebase --continue
    br = checkout -b
    wtf = !git-wtf
    ff = !git fetch && git wtf
    r = reset HEAD
    rh = reset --hard HEAD
    n = !"git ls-files | xargs notes | awk -F: '{ print $1,$2; print $3,$4; print $5}' | grcat conf.notes "
[push]
	default = tracking
[color]
    ui = true
[web]
	browser = open
[core]
  editor = vim --noplugin
[apply]
  whitespace = nowarn
